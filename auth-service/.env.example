# Application
APP_NAME=auth-service
APP_ENV=development
APP_PORT=8081
APP_DEBUG=true

# Database
DB_HOST=postgres
DB_PORT=5432
DB_NAME=auth_db
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL_MODE=disable

# JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRATION=24h

# Password
PASSWORD_SALT=your_password_salt

# Logging
LOG_LEVEL=debug

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080